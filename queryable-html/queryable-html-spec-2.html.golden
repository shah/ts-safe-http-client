<script>
  function footerToggle() {
  	if (viewport('portable')) {
  
  		if($("footer").hasClass('footer--mobile')){ 
  			if (footerActiveMenu.parent().hasClass('footer-level-active')) {
  				if(footerActiveMenu.parent().find('.footer-main--ul-level-2').hasClass('toggling')){return;}
  
  				footerActiveMenu.parent().find('.footer-main--ul-level-2').stop().slideUp(300,function(){
  					$('footer .footer-main--li-level-1').removeClass('toggling');
  					footerActiveMenu.parent().removeClass('footer-level-active');
  				});
  
  			} else {
  				if(footerActiveMenu.find('.footer-main--ul-level-2').hasClass('toggling')){return;}
  
  				footerActiveMenu.parent().find('.footer-main--ul-level-2').stop().slideDown(300,function(){
  					$('footer .footer-main--li-level-1').removeClass('toggling');
  				});
  				footerActiveMenu.parent().addClass('footer-level-active');
  			}
  		} 
  	} 
  }	
  
  var screenWidth = $(window).width();
  
  $(document).ready(function(){
  
  	$('.footer-main--li-level-1').click(function(event) {
  		footerActiveMenu = $(this);
  
  		if(screenWidth < 1024) {
  			$('footer').addClass('footer--mobile');
  			footerToggle()
  		}
  	});
  });	
  
  
  $(window).on('resize', function () {
      screenWidth = $(window).width();
  
      if (viewport("lap-and-up")) {
      	$('.footer-main--ul-level-2').css('display','');
  		$('.footer-main--ul-level-1').removeClass('footer-level-active');
  		$('footer').removeClass('footer--mobile');
  	}
  });
</script>




